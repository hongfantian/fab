<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/MainLayout" android:layout_width="800px"
	android:layout_gravity="center" android:layout_height="1200px"
	android:background="@drawable/homepage_background"
	android:soundEffectsEnabled="false">
	<FrameLayout android:layout_width="fill_parent"
		android:background="@drawable/home_bar" android:soundEffectsEnabled="false"
		android:layout_height="125px" android:id="@+id/StatusBarLayout">
		<ImageView android:background="@drawable/home_mylife_title"
			android:soundEffectsEnabled="false" android:layout_gravity="top|left"
			android:layout_width="150px" android:layout_height="53px"
			android:layout_marginTop="15px"></ImageView>
		<com.miinu.FabLife.ViewComponent.StatusBarLevel
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="188px" android:layout_height="53px"
			android:layout_marginTop="15px" android:id="@+id/StatusLevelView"
			android:layout_marginLeft="158px"></com.miinu.FabLife.ViewComponent.StatusBarLevel>
		<com.miinu.FabLife.ViewComponent.StatusBarEnergy
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="173px" android:layout_height="65px"
			android:layout_marginTop="10px" android:id="@+id/StatusEnergyView"
			android:layout_marginLeft="348px"></com.miinu.FabLife.ViewComponent.StatusBarEnergy>
		<com.miinu.FabLife.ViewComponent.StatusBarCoin
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="150px" android:layout_height="50px"
			android:layout_marginTop="18px" android:id="@+id/StatusCoinView"
			android:layout_marginLeft="523px"></com.miinu.FabLife.ViewComponent.StatusBarCoin>
		<com.miinu.FabLife.ViewComponent.StatusBarCash
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="125px" android:layout_height="50px"
			android:layout_marginTop="18px" android:id="@+id/StatusCashView"
			android:layout_marginLeft="675px"></com.miinu.FabLife.ViewComponent.StatusBarCash>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|left"
			android:textColor="@color/black" android:textSize="40px"
			android:layout_marginLeft="150px" android:soundEffectsEnabled="false"
			android:textScaleX="0.8" android:layout_marginTop="80px"
			android:id="@+id/StatusCaptionView"></TextView>
		<TextView android:layout_width="125px" android:gravity="left"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|center_horizontal"
			android:textColor="@color/black" android:layout_marginLeft="275px"
			android:textScaleX="0.8" android:layout_marginTop="80px"
			android:soundEffectsEnabled="false" android:textSize="40px"
			android:id="@+id/StatusTimerView"></TextView>
	</FrameLayout>
	<com.miinu.FabLife.ViewComponent.AvatarView
		android:layout_gravity="top|right" android:layout_height="950px"
		android:layout_width="600px" android:layout_marginRight="-150px"
		android:layout_marginTop="175px" android:id="@+id/MainAvatarView"
		android:soundEffectsEnabled="false"></com.miinu.FabLife.ViewComponent.AvatarView>
	<FrameLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:id="@+id/MainSubLayout"
		android:soundEffectsEnabled="false">
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Button_Job"
			android:background="@drawable/home_job_btn" android:layout_width="225px"
			android:layout_marginLeft="25px" android:layout_height="200px"
			android:layout_marginTop="200px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:layout_width="225px" android:layout_marginLeft="275px"
			android:id="@+id/Button_Shops" android:background="@drawable/home_shops_btn"
			android:layout_height="200px" android:layout_marginTop="200px"
			android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Button_Closet"
			android:background="@drawable/home_closet_btn" android:layout_width="225px"
			android:layout_marginLeft="25px" android:layout_height="200px"
			android:layout_marginTop="450px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Button_LookBook"
			android:background="@drawable/home_lookbook_btn"
			android:layout_width="225px" android:layout_marginLeft="275px"
			android:layout_height="200px" android:layout_marginTop="450px"
			android:soundEffectsEnabled="false"></ImageButton>
		<ImageView android:background="@drawable/home_exclamation"
			android:layout_gravity="top|left" android:layout_marginLeft="300px"
			android:layout_width="25px" android:layout_height="75px"
			android:id="@+id/ExclamationView" android:layout_marginTop="500px"
			android:soundEffectsEnabled="false"></ImageView>
		<ImageButton android:id="@+id/Button_MyBotique"
			android:background="@drawable/openboutique_btn"
			android:layout_height="200px" android:layout_width="225px"
			android:layout_gravity="top|left" android:layout_marginTop="700px"
			android:layout_marginLeft="25px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Button_Vip"
			android:background="@drawable/home_vip_btn" android:layout_width="225px"
			android:layout_marginLeft="275px" android:layout_height="200px"
			android:layout_marginTop="700px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:id="@+id/Button_Setting"
			android:background="@drawable/home_setting_btn"
			android:layout_marginLeft="25px" android:layout_gravity="bottom|left"
			android:layout_marginBottom="25px" android:soundEffectsEnabled="false"
			android:layout_width="120px" android:layout_height="100px"></ImageButton>
		<ImageButton android:id="@+id/Button_Energy"
			android:background="@drawable/home_energy_btn"
			android:layout_gravity="bottom|left" android:layout_marginBottom="25px"
			android:layout_marginLeft="150px" android:soundEffectsEnabled="false"
			android:layout_width="120px" android:layout_height="100px"></ImageButton>
		<ImageButton android:id="@+id/Button_Bank"
			android:background="@drawable/home_bank_btn" android:layout_gravity="bottom|left"
			android:layout_marginBottom="25px" android:layout_marginLeft="275px"
			android:soundEffectsEnabled="false" android:layout_width="120px"
			android:layout_height="100px"></ImageButton>
		<ImageButton android:background="@drawable/award"
			android:id="@+id/Button_Award" android:layout_gravity="bottom|left"
			android:layout_marginBottom="25px" android:layout_marginLeft="400px"
			android:soundEffectsEnabled="false" android:layout_height="100px"
			android:layout_width="120px"></ImageButton>
		<ImageButton android:background="@drawable/home_openfeint"
			android:id="@+id/Button_OpenFeint" android:layout_height="100px"
			android:layout_width="100px" android:layout_marginRight="25px"
			android:layout_gravity="bottom|right" android:layout_marginBottom="25px"
			android:soundEffectsEnabled="false"></ImageButton>
	</FrameLayout>
	<ImageView android:layout_gravity="top|left"
		android:layout_marginLeft="275px" android:background="@drawable/clickit1"
		android:id="@+id/ClickItView01" android:layout_height="175px"
		android:layout_width="275px" android:layout_marginTop="230px"
		android:visibility="gone" android:soundEffectsEnabled="false"></ImageView>
	<ImageView android:layout_gravity="top|left"
		android:layout_height="175px" android:background="@drawable/clickit2"
		android:id="@+id/ClickItView02" android:layout_marginLeft="25px"
		android:layout_width="250px" android:layout_marginTop="230px"
		android:visibility="gone" android:soundEffectsEnabled="false"></ImageView>
	<ImageView android:layout_gravity="top|left"
		android:layout_marginLeft="275px" android:background="@drawable/clickit1"
		android:layout_height="175px" android:layout_width="275px" android:id="@+id/ClickItView03"
		android:layout_marginTop="480px" android:visibility="gone"
		android:soundEffectsEnabled="false"></ImageView>
	<ImageView android:background="@drawable/it_hint"
		android:layout_width="500px" android:layout_height="450px"
		android:layout_gravity="top|left" android:id="@+id/HintView"
		android:layout_marginLeft="55px" android:layout_marginTop="575px"
		android:visibility="gone" android:soundEffectsEnabled="false"></ImageView>
</FrameLayout>
