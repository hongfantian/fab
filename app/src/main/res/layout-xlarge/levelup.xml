<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="800px" android:layout_height="1200px"
	android:layout_gravity="center">
	<ImageView android:background="@drawable/levelup"
		android:layout_gravity="top|center_horizontal" android:layout_width="700px"
		android:layout_height="500px" android:id="@+id/LevelUpBgView" android:layout_marginTop="388px"></ImageView>
	<TextView android:textColor="@color/white" android:gravity="center"
		android:layout_gravity="top|center_horizontal" android:layout_width="600px" android:layout_height="200px"
		android:id="@+id/LevelUpMsgView" android:textScaleX="0.8" android:textSize="55px" android:layout_marginTop="538px"></TextView>
	<ImageButton android:background="@drawable/levelup_ok"
		android:layout_gravity="top|center_horizontal" android:id="@+id/LevelUpOkButton" android:soundEffectsEnabled="false" android:layout_width="175px" android:layout_height="95px" android:layout_marginTop="763px"></ImageButton>
</FrameLayout>
