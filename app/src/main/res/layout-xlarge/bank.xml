<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_gravity="center" android:layout_height="1200px"
	android:layout_width="800px" android:soundEffectsEnabled="false"
	android:background="@drawable/bank_background">
	<FrameLayout android:layout_width="fill_parent"
		android:background="@drawable/home_bar" android:soundEffectsEnabled="false"
		android:layout_height="125px" android:id="@+id/StatusBarLayout">
		<ImageView android:soundEffectsEnabled="false"
			android:layout_gravity="top|left" android:background="@drawable/home_bank_title"
			android:layout_width="110px" android:layout_height="45px"
			android:layout_marginTop="20px" android:layout_marginLeft="20px"></ImageView>
		<com.miinu.FabLife.ViewComponent.StatusBarLevel
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="188px" android:layout_height="53px"
			android:layout_marginTop="15px" android:id="@+id/StatusLevelView"
			android:layout_marginLeft="158px"></com.miinu.FabLife.ViewComponent.StatusBarLevel>
		<com.miinu.FabLife.ViewComponent.StatusBarEnergy
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="173px" android:layout_height="65px"
			android:layout_marginTop="10px" android:id="@+id/StatusEnergyView"
			android:layout_marginLeft="348px"></com.miinu.FabLife.ViewComponent.StatusBarEnergy>
		<com.miinu.FabLife.ViewComponent.StatusBarCoin
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="150px" android:layout_height="50px"
			android:layout_marginTop="18px" android:id="@+id/StatusCoinView"
			android:layout_marginLeft="523px"></com.miinu.FabLife.ViewComponent.StatusBarCoin>
		<com.miinu.FabLife.ViewComponent.StatusBarCash
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="125px" android:layout_height="50px"
			android:layout_marginTop="18px" android:id="@+id/StatusCashView"
			android:layout_marginLeft="675px"></com.miinu.FabLife.ViewComponent.StatusBarCash>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|left"
			android:textColor="@color/black" android:textSize="40px"
			android:layout_marginLeft="150px" android:soundEffectsEnabled="false"
			android:textScaleX="0.8" android:layout_marginTop="80px"
			android:id="@+id/StatusCaptionView"></TextView>
		<TextView android:layout_width="125px" android:gravity="left"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|center_horizontal"
			android:textColor="@color/black" android:layout_marginLeft="275px"
			android:textScaleX="0.8" android:layout_marginTop="80px"
			android:soundEffectsEnabled="false" android:textSize="40px"
			android:id="@+id/StatusTimerView"></TextView>
	</FrameLayout>
	<com.miinu.FabLife.ViewComponent.AvatarView
		android:layout_gravity="top|right" android:layout_marginTop="175px"
		android:layout_height="950px" android:layout_marginRight="-150px"
		android:layout_width="600px" android:id="@+id/BankAvatarView"
		android:soundEffectsEnabled="false"></com.miinu.FabLife.ViewComponent.AvatarView>
	<FrameLayout android:animationCache="false" android:layout_marginTop="125px"
		android:layout_width="800px" android:layout_gravity="top|center_horizontal"
		android:layout_height="1075px" android:soundEffectsEnabled="false" android:layoutAnimation="@anim/layout_animation_right_slide">
		<ImageView android:background="@drawable/bank_title"
			android:id="@+id/BankTitleView" android:layout_gravity="top|left"
			android:layout_marginLeft="25px" android:layout_marginTop="75px"
			android:layout_width="433px" android:layout_height="90px"
			android:soundEffectsEnabled="false"></ImageView>
		<ImageButton android:layout_gravity="top|left"
			android:background="@drawable/cash_20" android:id="@+id/Cash20Button"
			android:layout_marginLeft="25px" android:layout_marginTop="250px"
			android:soundEffectsEnabled="false" android:layout_width="158px"
			android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Cash53Button"
			android:background="@drawable/cash_53" android:layout_marginLeft="188px"
			android:layout_marginTop="250px" android:soundEffectsEnabled="false"
			android:layout_height="155px" android:layout_width="160px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:layout_marginLeft="350px" android:background="@drawable/cash_110"
			android:id="@+id/Cash110Button" android:layout_marginTop="250px"
			android:soundEffectsEnabled="false" android:layout_width="160px"
			android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Cash288Button"
			android:background="@drawable/cash_288" android:layout_marginLeft="25px"
			android:layout_marginTop="425px" android:soundEffectsEnabled="false"
			android:layout_width="163px" android:layout_height="158px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:background="@drawable/cash_600" android:id="@+id/Cash600Button"
			android:layout_marginLeft="188px" android:layout_marginTop="425px"
			android:soundEffectsEnabled="false" android:layout_width="158px"
			android:layout_height="158px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:layout_marginLeft="350px" android:id="@+id/Cash1270Button"
			android:background="@drawable/cash_1270" android:layout_marginTop="425px"
			android:soundEffectsEnabled="false" android:layout_width="165px"
			android:layout_height="158px"></ImageButton>

		<ImageButton android:layout_gravity="top|left"
			android:background="@drawable/coin_400" android:id="@+id/Coin400Button"
			android:layout_marginLeft="25px" android:layout_marginTop="650px"
			android:soundEffectsEnabled="false" android:layout_width="165px"
			android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Coin1050Button"
			android:background="@drawable/coin_1050" android:layout_marginLeft="188px"
			android:layout_marginTop="650px" android:soundEffectsEnabled="false"
			android:layout_width="165px" android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:layout_marginLeft="350px" android:background="@drawable/coin_2200"
			android:id="@+id/Coin2200Button" android:layout_marginTop="650px"
			android:soundEffectsEnabled="false" android:layout_width="165px"
			android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left" android:id="@+id/Coin5750Button"
			android:background="@drawable/coin_5750" android:layout_marginLeft="25px"
			android:layout_marginTop="825px" android:soundEffectsEnabled="false"
			android:layout_width="165px" android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:background="@drawable/coin_12000" android:id="@+id/Coin12000Button"
			android:layout_marginLeft="188px" android:layout_marginTop="825px"
			android:soundEffectsEnabled="false" android:layout_width="165px"
			android:layout_height="155px"></ImageButton>
		<ImageButton android:layout_gravity="top|left"
			android:layout_marginLeft="350px" android:id="@+id/Coin25500Button"
			android:background="@drawable/coin_25500" android:layout_marginTop="825px"
			android:soundEffectsEnabled="false" android:layout_width="165px"
			android:layout_height="155px"></ImageButton>

		<ImageButton android:background="@drawable/gohome_btn"
			android:layout_gravity="bottom|right" android:layout_marginRight="50px"
			android:layout_marginBottom="50px" android:id="@+id/BankGoHomeButton"
			android:soundEffectsEnabled="false" android:layout_height="110px"
			android:layout_width="110px"></ImageButton>
	</FrameLayout>
</FrameLayout>
