		<ImageView android:layout_marginLeft="169px"
			android:background="@drawable/job_hint" android:layout_height="188px"
			android:layout_marginTop="188px" android:id="@+id/ImageView01"
			android:layout_width="341px" android:layout_gravity="top|left"
			android:visibility="gone"></ImageView>
	</FrameLayout>
</FrameLayout>
