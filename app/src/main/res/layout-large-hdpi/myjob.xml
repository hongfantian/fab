				android:textColor="@color/black" android:layout_gravity="top|left"
				android:gravity="center" android:id="@+id/Energy03"
				android:textSize="23px" android:layout_height="wrap_content"
				android:layout_width="wrap_content" android:textScaleX="0.8" android:layout_marginTop="45px" android:layout_marginLeft="98px"></TextView>
			<TextView android:editable="false" android:singleLine="true"
				android:textColor="@color/black" android:layout_gravity="top|left"
				android:gravity="center" android:id="@+id/Coin03" android:textSize="23px"
				android:layout_height="wrap_content" android:layout_width="wrap_content"
				android:textScaleX="0.8" android:layout_marginTop="75px" android:layout_marginLeft="98px"></TextView>
			<com.miinu.FabLife.ViewComponent.CustomProgress
				android:layout_gravity="top|left" android:layout_width="150px"
				android:layout_marginLeft="158px" android:id="@+id/Progress03"
				android:layout_marginTop="45px" android:layout_height="75px"></com.miinu.FabLife.ViewComponent.CustomProgress>
			<ImageButton android:layout_height="94px"
				android:layout_width="94px" android:layout_marginLeft="308px"
