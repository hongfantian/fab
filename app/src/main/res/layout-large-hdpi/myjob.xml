		<ImageButton android:layout_gravity="bottom|left"
			android:id="@+id/Level9Button" android:layout_marginBottom="19px"
			android:layout_height="75px" android:layout_width="64px"
			android:layout_marginLeft="161px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="bottom|left"
			android:id="@+id/Level13Button" android:layout_height="75px"
			android:layout_marginBottom="19px" android:layout_width="64px"
			android:layout_marginLeft="233px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="bottom|left"
			android:id="@+id/Level17Button" android:layout_height="75px"
			android:layout_marginBottom="19px" android:layout_width="64px"
			android:layout_marginLeft="304px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_gravity="bottom|left"
			android:layout_height="75px" android:layout_marginBottom="19px"
			android:layout_width="64px" android:id="@+id/Level26Button"
			android:layout_marginLeft="375px" android:soundEffectsEnabled="false"></ImageButton>
		<ImageButton android:layout_height="94px"
			android:layout_width="94px" android:layout_gravity="bottom|right"
			android:layout_marginBottom="19px" android:layout_marginRight="19px"
			android:background="@drawable/gohome_btn" android:id="@+id/MyJobGoHomeButton"
