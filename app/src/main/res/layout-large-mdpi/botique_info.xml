<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_gravity="center" android:layout_height="1020px"
	android:layout_width="680px" android:soundEffectsEnabled="false">
	<FrameLayout android:layout_marginTop="106px"
		android:layout_width="680px" android:layout_gravity="top|center_horizontal"
		android:layout_height="914px" android:soundEffectsEnabled="false">
		<ImageView android:id="@+id/BotiqueTipView"
			android:background="@drawable/myboutiqueinfo" android:layout_gravity="center"
			android:layout_width="638px" android:layout_height="489px" android:soundEffectsEnabled="false"></ImageView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/BotiqueEarningView"
			android:textColor="@color/white" android:layout_gravity="top|left" android:layout_marginTop="361px"
			android:layout_marginLeft="391px" android:soundEffectsEnabled="false" android:textSize="43px" android:textScaleX="0.8"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textColor="@color/white"
			android:layout_gravity="top|left"
			android:layout_marginLeft="391px" android:id="@+id/BotiqueStoreView" android:soundEffectsEnabled="false" android:textSize="43px" android:textScaleX="0.8" android:layout_marginTop="451px"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textColor="@color/white"
			android:layout_gravity="top|left"
			android:layout_marginLeft="391px" android:id="@+id/BotiqueCustomerView" android:soundEffectsEnabled="false" android:layout_marginTop="538px" android:textSize="43px" android:textScaleX="0.8"></TextView>
		<ImageButton android:background="@drawable/botiqueinfo_ok"
			android:layout_marginTop="595px" android:layout_gravity="top|right"
			android:layout_marginRight="85px" android:layout_height="85px"
			android:id="@+id/BotiqueInfoOkButton" android:layout_width="145px" android:soundEffectsEnabled="false"></ImageButton>
	</FrameLayout>
</FrameLayout>
