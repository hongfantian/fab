<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_gravity="center" android:layout_height="1020px"
	android:layout_width="680px" android:soundEffectsEnabled="false"
	android:background="@drawable/cafe_background">
	<FrameLayout android:layout_width="fill_parent"
		android:background="@drawable/home_bar" android:soundEffectsEnabled="false"
		android:layout_height="106px" android:id="@+id/StatusBarLayout">
		<ImageView android:soundEffectsEnabled="false"
			android:layout_gravity="top|left" android:background="@drawable/home_cafe_title"
			android:layout_width="132px" android:layout_height="40px"
			android:layout_marginTop="17px"></ImageView>
		<com.miinu.FabLife.ViewComponent.StatusBarLevel
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="159px" android:layout_height="45px"
			android:layout_marginTop="13px" android:id="@+id/StatusLevelView"
			android:layout_marginLeft="134px"></com.miinu.FabLife.ViewComponent.StatusBarLevel>
		<com.miinu.FabLife.ViewComponent.StatusBarEnergy
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="147px" android:layout_height="55px"
			android:layout_marginTop="9px" android:id="@+id/StatusEnergyView"
			android:layout_marginLeft="295px"></com.miinu.FabLife.ViewComponent.StatusBarEnergy>
		<com.miinu.FabLife.ViewComponent.StatusBarCoin
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="128px" android:layout_height="43px"
			android:layout_marginTop="15px" android:id="@+id/StatusCoinView"
			android:layout_marginLeft="444px"></com.miinu.FabLife.ViewComponent.StatusBarCoin>
		<com.miinu.FabLife.ViewComponent.StatusBarCash
			android:layout_gravity="top|left" android:soundEffectsEnabled="false"
			android:layout_width="106px" android:layout_height="43px"
			android:layout_marginTop="15px" android:id="@+id/StatusCashView"
			android:layout_marginLeft="574px"></com.miinu.FabLife.ViewComponent.StatusBarCash>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|left"
			android:textColor="@color/black" android:textSize="34px"
			android:layout_marginLeft="128px" android:soundEffectsEnabled="false"
			android:textScaleX="0.8" android:layout_marginTop="68px"
			android:id="@+id/StatusCaptionView"></TextView>
		<TextView android:layout_width="106px" android:gravity="left"
			android:layout_height="wrap_content" android:editable="false"
			android:singleLine="true" android:layout_gravity="top|center_horizontal"
			android:textColor="@color/black" android:layout_marginLeft="234px"
			android:textScaleX="0.8" android:layout_marginTop="68px"
			android:soundEffectsEnabled="false" android:textSize="34px"
			android:id="@+id/StatusTimerView"></TextView>
	</FrameLayout>
	<com.miinu.FabLife.ViewComponent.AvatarView
		android:layout_gravity="top|right" android:layout_marginTop="149px"
		android:layout_height="808px" android:layout_marginRight="-128px"
		android:layout_width="510px" android:id="@+id/CafeAvatarView"
		android:soundEffectsEnabled="false"></com.miinu.FabLife.ViewComponent.AvatarView>
	<FrameLayout android:animationCache="false"
		android:layout_marginTop="106px" android:layout_gravity="top|center_horizontal"
		android:layout_width="680px" android:layout_height="914px"
		android:soundEffectsEnabled="false">
		<FrameLayout android:layoutAnimation="@anim/layout_animation_right_slide"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:layout_gravity="top" android:layout_marginTop="43px"
			android:soundEffectsEnabled="false">
			<ImageView android:id="@+id/ImageView01"
				android:background="@drawable/cafe_title" android:layout_gravity="top|left"
				android:layout_marginLeft="43px" android:layout_width="332px"
				android:layout_height="47px"></ImageView>
		</FrameLayout>
		<FrameLayout android:layoutAnimation="@anim/layout_animation_right_slide"
			android:layout_marginTop="149px" android:layout_gravity="top"
			android:layout_height="wrap_content" android:layout_width="fill_parent">
			<ImageButton android:background="@drawable/strawberry"
				android:id="@+id/StrawberryButton" android:layout_gravity="left"
				android:layout_marginLeft="21px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="134px"></ImageButton>
			<ImageButton android:background="@drawable/espresso"
				android:id="@+id/EspressoButton" android:layout_gravity="left"
				android:layout_marginLeft="255px" android:soundEffectsEnabled="false"
				android:layout_width="219px" android:layout_height="134px"></ImageButton>
		</FrameLayout>
		<FrameLayout android:layoutAnimation="@anim/layout_animation_left_slide"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:layout_gravity="top" android:layout_marginTop="319px">
			<ImageButton android:background="@drawable/milkshake"
				android:id="@+id/MilkshakeButton" android:layout_gravity="left"
				android:layout_marginLeft="21px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="132px"></ImageButton>
			<ImageButton android:id="@+id/HamburgerButton"
				android:background="@drawable/hamburger" android:layout_gravity="left"
				android:layout_marginLeft="255px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="132px"></ImageButton>
		</FrameLayout>
		<FrameLayout android:layoutAnimation="@anim/layout_animation_right_slide"
			android:layout_height="wrap_content" android:layout_width="fill_parent"
			android:layout_gravity="top" android:layout_marginTop="489px">
			<ImageButton android:background="@drawable/sushi"
				android:id="@+id/SushiButton" android:layout_gravity="left"
				android:layout_marginLeft="21px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="136px"></ImageButton>
			<ImageButton android:background="@drawable/cheesecake"
				android:id="@+id/CheesecakeButton" android:layout_marginLeft="255px"
				android:layout_gravity="left" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="136px"></ImageButton>
		</FrameLayout>
		<FrameLayout android:layoutAnimation="@anim/layout_animation_left_slide"
			android:layout_gravity="top" android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:layout_marginTop="659px">
			<ImageButton android:background="@drawable/chocolate"
				android:id="@+id/ChocolateButton" android:layout_gravity="left"
				android:layout_marginLeft="21px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="134px"></ImageButton>
			<ImageButton android:background="@drawable/macaron"
				android:id="@+id/MacaronButton" android:layout_gravity="left"
				android:layout_marginLeft="255px" android:soundEffectsEnabled="false"
				android:layout_width="223px" android:layout_height="132px"></ImageButton>
		</FrameLayout>
		<FrameLayout android:layout_height="wrap_content"
			android:layout_width="fill_parent" android:layout_gravity="bottom"
			android:layout_marginBottom="43px" android:layoutAnimation="@anim/layout_animation_right_slide">
			<ImageButton android:background="@drawable/gohome_btn"
				android:id="@+id/CafeGoHomeButton" android:layout_marginRight="43px"
				android:layout_gravity="right" android:soundEffectsEnabled="false"
				android:layout_width="94px" android:layout_height="94px"></ImageButton>
		</FrameLayout>
	</FrameLayout>
</FrameLayout>
